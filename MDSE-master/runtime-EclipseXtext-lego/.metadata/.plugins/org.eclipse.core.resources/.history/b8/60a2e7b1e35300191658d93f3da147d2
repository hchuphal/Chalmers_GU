BrickModel model {
    limits Dimensions {
        width 20
        height 20
    }
    
    abstract Brick D1 {
        Dimensions
            height 2
            width 4      
           }
    abstract Brick D2 {
        Dimensions
        height 2
        width 3
    }
    abstract Brick D3 {
        Dimensions
        height 2
        width 3
        
    }
    
    
    Brick D4 {
        createBrick{
            Dimensions(D1(2,1), D2(1,1))
            
        } 
         
    }
}